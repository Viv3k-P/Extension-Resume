{
  "manifest_version": 2,
  "name": "Text Highlighter to API",
  "version": "1.4",
  "permissions": [
    "contextMenus",
    "activeTab",
    "notifications",
    "storage",
    "http://localhost:5000/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Text Highlighter"
  },
  "icons": {
    "48": "icon.png"
  }
}